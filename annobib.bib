@article{ ISI:000179909600012,
Author = {Pascual-Marqui, RD and Esslen, M and Kochi, K and Lehmann, D},
Title = {{Functional imaging with low-resolution brain electromagnetic tomography
   (LORETA): A review}},
Journal = {{METHODS AND FINDINGS IN EXPERIMENTAL AND CLINICAL PHARMACOLOGY}},
Year = {{2002}},
Volume = {{24}},
Number = {{C}},
Pages = {{91-95}},
Note = {{12th Meeting of the International-Pharmaco-EEG-Group, BARCELONA, SPAIN,
   NOV 21-24, 2002}},
Organization = {{Int Pharmaco EEG Grp}},
Abstract = {{This paper reviews several recent publications that have successfully
   used the functional brain imaging method known as LORETA. Emphasis is
   placed oil the electrophysiological and neuroanatomical basis of the
   method, on the localization properties of the method, and oil the
   validation of the method in real experimental human data. Papers that
   criticize LORETA are briefly discussed. LORETA publications in the
   1994-1997 period based localization inference on images of raw electric
   neuronal activity. In 1998, a series of papers appeared that based
   localization inference on the statistical parametric mapping methodology
   applied to high-time resolution LORETA images. Starting in 1999,
   quantitative neuroanotomy was added to the methodology based oil the
   digitized Talairach atlas provided by the Brain Imaging Centre, Montreal
   Neurological Institute. The combination of these methodological
   developments has placed LORETA at a level that compares favorably to the
   more classical functional imaging methods, such as PET and fMRI. (C)
   2002 Prous Science. All rights reserved.}},
